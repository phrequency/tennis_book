<div class="container" align="center">
	<div class="row">
		<div class="col-md-6">
			<div class="dash-title">
				Get my results
			</div>
			<%= form_tag(:action => "get_usta_data", :controller => "users") do %>
				<div><%= label_tag :first_name %></div>
				<div><%= text_field_tag :first_name %></div>
				<div><%= label_tag :last_name %></div>
				<div><%= text_field_tag :last_name %></div>
				<%= submit_tag "Search" %>
			<% end %>
		</div>
		<div class="col-md-6">
			<div class="dash-title">
				Search for an opponent
			</div>
		</div>
	</div>
</div>